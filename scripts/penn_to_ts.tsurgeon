%%%%%%%%%%%%%%%%%%%%%%%%
% IP-IMP
IP-SMC-IMP=x

relabel x IP-IMP


%%%%%%%%%%%%%%%%%%%%%%%%
% move SBJ out of INF
/^IP-INF/=x !== /\bREL\b/ <1 /^NP-SBJ\b(.*)$/`#'1\%extra=y

move y $+ x
relabel y /^.*$/NP-DOB1\%{extra}/


%%%%%%%%%%%%%%%%%%%%%%%%
% NML changes
NML=x $ CONJP > _NP

relabel x NP


NP <: NML=x

excise x x


%%%%%%%%%%%%%%%%%%%%%%%%
% remove layers under part-of-speech
/^(CONJ|P-ROLE)$/ < (__=x < __)

excise x x


%%%%%%%%%%%%%%%%%%%%%%%%
% merge words
__=x !< __ $. (__=y !< __)

relabel x /^.*$/={x}_={y}/
delete y


%%%%%%%%%%%%%%%%%%%%%%%%
% definite marking
D=x < /^(TH|Th|th)/

relabel x /D;{DEF}/


